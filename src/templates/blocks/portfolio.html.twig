<div class="section grid-x grid-padding-x grid-padding-y grid-margin-y medium-up-2 large-up-3 align-center">
    {% for demo in demos %}
        <section class="cell small-6 large-4">
            <a class="card" href="/demos/{{ demo.title }}" title="Demo {{ demo.title }}" data-aos="fade-up">
                <div class="card__image">
                    <img class="card__image__img" src="/img/{{ demo.image }}" alt=""/>
                </div>
            </a>
        </section>
    {% endfor %}
</div>
